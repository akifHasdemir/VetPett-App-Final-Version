<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="600.0" prefWidth="800.0" style="-fx-background-color: dddddd;" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="vetApplication.vetAppointmentController">
   <children>
      <VBox prefHeight="600.0" prefWidth="200.0">
         <children>
            <Label text="VetPet">
               <VBox.margin>
                  <Insets left="40.0" top="50.0" />
               </VBox.margin>
               <font>
                  <Font size="36.0" />
               </font>
            </Label>
            <Button mnemonicParsing="false" onAction="#goToVetHomePage" text="Home Page">
               <font>
                  <Font size="16.0" />
               </font>
               <VBox.margin>
                  <Insets left="40.0" top="300.0" />
               </VBox.margin>
            </Button>
         </children>
      </VBox>
      <VBox layoutX="200.0" prefHeight="600.0" prefWidth="600.0">
         <children>
            <HBox prefHeight="100.0" prefWidth="600.0">
               <children>
                  <Label text="Appointments">
                     <font>
                        <Font size="30.0" />
                     </font>
                     <HBox.margin>
                        <Insets left="200.0" top="20.0" />
                     </HBox.margin>
                  </Label>
               </children>
            </HBox>
            <ScrollPane prefHeight="500.0" prefWidth="600.0">
               <content>
                  <VBox fx:id="vetAppointmentBox" prefHeight="500.0" prefWidth="600.0" />
               </content>
            </ScrollPane>
         </children>
      </VBox>
   </children>
</AnchorPane>
